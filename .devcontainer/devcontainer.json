{
  "name": "Java Dev Container",
  "dockerComposeFile": "docker-compose.yml",
  "service": "dev",
  "workspaceFolder": "/workspace/app/mydockersample",
  "remoteUser": "vscode",
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack",
        "pivotal.vscode-spring-boot",
        "eamodio.gitlens"
      ]
    }
  },
  "runServices": ["dev"],
  "shutdownAction": "stopCompose"
}
